game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Checking Game", Text = "Please Wait"})
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
        wait(2)
        game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Zombie Attack", Text = "Checking Done"})
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
       wait(2)
        game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Loading UI", Text = "Please Wait 2 Seconds"})
       wait(2)
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()       
local lib = loadstring(game:HttpGet('https://raw.githubusercontent.com/vuthaison1308/vuthaison1308/main/TEST'))()
local window = lib:Window('TheaugHub')

window:Toggle('Hitbox Expanding', function(enabled)
     loadstring(game:HttpGet('https://raw.githubusercontent.com/vuthaison1308/vuthaison1308/main/NPChitbox/ByTheaug'))()
      wait(1)
     game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Press F9", Text = "To see supported game"})
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
print"Supported Game:"
print"-Be A Parkour Ninja"
print"-Slap Battles"
print"-Zombie Attack"
end)
window:Toggle('Dupe Gun Bullets', function(enabled)
     local rac = require(game:GetService("ReplicatedStorage").GunData)
for k,v in pairs(rac) do 
    v.Firerate=1000
    v.Automatic=true
    v.ReloadTime=0
end
game.Players.LocalPlayer.Humanoid.Health=0
end)
window:Toggle('Ctrl + Click = Teleport', function(enabled)
     CTRL = true
    local Plr = game:GetService("Players").LocalPlayer
    local Mouse = Plr:GetMouse()
    Mouse.Button1Down:connect(
        function()
            if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then
                return
            end
            if not Mouse.Target then
                return
            end
            if CTRL then
                Plr.Character:MoveTo(Mouse.Hit.p)
            end
        end)
     end)  
window:Toggle('Kick Me From The Server', function(enabled)
     game.Players.LocalPlayer:kick("Got Kicked By Press The Button")
     end)  
window:Toggle('Close Roblox', function(enabled)
     game:Shutdown()
     end)
