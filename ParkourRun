getgenv().HopDelay = nil -- nhap thoi gian delay hop (mac dinh = 2)
repeat wait() until game:IsLoaded() and game.Players.LocalPlayer
if getgenv().HopDelay == nil then
    getgenv().HopDelay = 2
end
spawn(function()
    while wait() do
        while wait() do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").CurrentStages.RunEnd.EndWall.Position)
        end
    end
end)
spawn(function()
    wait(getgenv().HopDelay)
    local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/vuthaison1308/vuthaison1308/main/hopserver")()
    module:Teleport(game.PlaceId)
end)
