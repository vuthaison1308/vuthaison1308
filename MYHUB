local PlaceId = game.PlaceId
if PlaceId == 6403373529 then
        game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Checking Game", Text = "Please Wait"})
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
        wait(2)
        game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Slap Battles", Text = "Checking Done"})
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
       wait(2)
        game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Loading UI", Text = "Please Wait 2 Seconds"})
       wait(2)
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()       
local lib = loadstring(game:HttpGet('https://raw.githubusercontent.com/vuthaison1308/vuthaison1308/main/TEST'))()
local window = lib:Window('TheaugHub')

window:Toggle('Hitbox Expanding', function(enabled)
     loadstring(game:HttpGet"https://raw.githubusercontent.com/vuthaison1308/vuthaison1308/main/SlapBattles")()
     wait(1)
     game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Press F9", Text = "To see supported game"})
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
print"Supported Game:"
print"-Be A Parkour Ninja"
print"-Slap Battles"
end)
window:Toggle('Hop Random Server', function(enabled)
     local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/147848991/servers/Public?sortOrder=Asc&limit=100"))
for i,v in pairs(Servers.data) do
  if v.playing ~= v.maxPlayers then
      game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
  end
end
     end)
window:Toggle('Kick Me From The Server', function(enabled)
     game.Players.LocalPlayer:kick("Got Kicked By Press The Button")
     end)  
window:Toggle('Close Roblox', function(enabled)
     game:Shutdown()
     end)
 elseif PlaceId == 147848991 then
                game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Checking Game", Text = "Please Wait"})
